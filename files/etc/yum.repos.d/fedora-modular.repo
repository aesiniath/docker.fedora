[fedora-modular]
name=Fedora Modular $releasever - $basearch - Release
failovermethod=priority
baseurl=http://packages/fedora/linux/releases/$releasever/Modular/$basearch/os/
enabled=1
metadata_expire=7d
repo_gpgcheck=0
type=rpm
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$releasever-$basearch
skip_if_unavailable=False
